{
  "name": "commit-prehook-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "commit-msg": "^0.2.3",
    "pre-commit": "^1.2.2"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "jira-prepare-commit-msg": "^1.6.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karolguzik/precommit-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karolguzik/precommit-test/issues"
  },
  "homepage": "https://github.com/karolguzik/precommit-test#readme",
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J]\n$M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": false
  }
}
